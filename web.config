<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <directoryBrowse enabled="false"/>
    <rewrite>
      <rewriteMaps>
        <rewriteMap name="index.php"/>
      </rewriteMaps>
      <rules><rule name="Rewrite rule1 for index.php"><match url=".*"/><conditions><add input="{index.php:{REQUEST_FILENAME}}" pattern="(.+)"/></conditions><action type="Rewrite" url="index.php" appendQueryString="false"/></rule>
			<rule name="WordPress: http://cancapital" patternSyntax="Wildcard">
				<match url="*"/>
					<conditions>
						<add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true"/>
						<add input="{REQUEST_FILENAME}" matchType="IsDirectory" negate="true"/>
					</conditions>
				<action type="Rewrite" url="index.php"/>
			</rule></rules>
    </rewrite>
  </system.webServer>
</configuration>
